    "from enum import Enum\n",
    "\n",
    "class TemporalType(Enum):\n",
    "    \n",
    "    ATEMPORAL = \"ATEMPORAL\"\n",
    "    STATIC = \"STATIC\"\n",
    "    DYNAMIC = \"DYNAMIC\"\n",
    "\n",
    "class StatementType(Enum):\n",
    "    FACT = \"FACT\"\n",
    "    OPINION = \"OPINION\"\n",
    "    PREDICTION = \"PREDICTION\""
   ]
  },
  {
